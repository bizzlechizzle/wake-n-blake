{
  "name": "wake-n-blake",
  "version": "0.1.0",
  "description": "Universal BLAKE3 hashing, verification, and ID generation CLI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "wnb": "./bin/wnb.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "blake3",
    "hash",
    "checksum",
    "uuid",
    "ulid",
    "file-integrity",
    "verification",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blake3": "^2.1.7",
    "commander": "^12.1.0",
    "ignore": "^6.0.2",
    "ora": "^8.1.1",
    "p-queue": "^8.0.1",
    "ulid": "^2.3.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
